B,kBAAA,KAAK,EAAE,KAAI,CAAC,KAAL,CAAW,KAHE;AAIpB,kBAAA,QAAQ,EAAE,KAAI,CAAC,QAAL,CAAc;AAJJ,iBAVL,EAiBjB;;AAjBiB;AAAA;AAAA,uBAmBT,KAAI,CAAC,MAAL,CAAY,QAAZ,CAAqB,UAArB,EAAiC,aAAjC,CAnBS;;AAAA;AAoBT,gBAAA,WApBS,GAoBK,OAAO,KAAI,CAAC,MAAL,CAAY,KAAZ,CAAkB,QAAlB,IAA8B,QAArC,CApBL;;AAqBf,gBAAA,KAAI,CAAC,OAAL,CAAa,OAAb,CAAqB,WAArB;;AArBe;AAAA;;AAAA;AAAA;AAAA;AAuBf,iCACE,KAAI,CAAC,KAAL,GACE,sBAAsB,aAAa,CAAC,KAApC,GAA4C,iBAA5C,IACA,oBAFF,CADF,KAIK,KAAI,CAAC,KAAL,GAAa,oBAAb;;AA3BU;AA8BjB,gBAAA,KAAI,CAAC,SAAL,GAAiB,KAAjB;;AA9BiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA+BlB,KAhCM;AAkCP,IAAA,cAlCO,4BAkCU;AACf,WAAK,WAAL,GAAmB,IAAnB;AACA,WAAK,IAAL,CAAU,OAAV,GAAoB,IAApB;;AACA,UAAI,KAAK,IAAL,CAAU,KAAV,KAAoB,EAAxB,EAA4B;AAC1B,aAAK,WAAL,GAAmB,KAAnB;AACA,aAAK,IAAL,CAAU,OAAV,GAAoB,KAApB;AACF;AACD,KAzCM;AA0CP,IAAA,iBA1CO,+BA0Ca;AAClB,WAAK,WAAL,GAAmB,IAAnB;AACA,WAAK,OAAL,CAAa,OAAb,GAAuB,IAAvB;;AACA,UAAI,KAAK,OAAL,CAAa,KAAb,KAAuB,EAA3B,EAA+B;AAC7B,aAAK,WAAL,GAAmB,KAAnB;AACA,aAAK,OAAL,CAAa,OAAb,GAAuB,KAAvB;AACF;AACD,KAjDM;AAmDP,IAAA,eAnDO,6BAmDW;AAChB,WAAK,KAAL,CAAW,OAAX,GAAqB,IAArB;AACA,WAAK,WAAL,GAAmB,IAAnB;;AACA,UAAI,CAAC,KAAK,KAAL,CAAW,KAAZ,IAAqB,CAAC,KAAK,KAAL,CAAW,KAAX,CAAiB,QAAjB,CAA0B,GAA1B,CAA1B,EAA0D;AACxD,aAAK,KAAL,CAAW,OAAX,GAAqB,KAArB;AACA,aAAK,WAAL,GAAmB,KAAnB;AACF;AACD,KA1DM;AA2DP,IAAA,kBA3DO,gCA2Dc;AACnB,WAAK,WAAL,GAAmB,IAAnB;AACA,WAAK,QAAL,CAAc,OAAd,GAAwB,IAAxB;;AACA,UAAI,CAAC,KAAK,QAAL,CAAc,KAAf,IAAwB,KAAK,QAAL,CAAc,KAAd,CAAoB,MAApB,GAA6B,CAAzD,EAA4D;AAC1D,aAAK,WAAL,GAAmB,KAAnB;AACA,aAAK,QAAL,CAAc,OAAd,GAAwB,KAAxB;AACF;AACD,KAlEM;AAmEP,IAAA,iBAnEO,+BAmEa;AAClB,WAAK,WAAL,GAAmB,IAAnB;AACA,WAAK,OAAL,CAAa,OAAb,GAAuB,IAAvB;;AACA,UACE,CAAC,KAAK,OAAL,CAAa,KAAd,IACA,KAAK,OAAL,CAAa,KAAb,GAAqB,CADrB,IAEA,KAAK,OAAL,CAAa,KAAb,KAAuB,KAAK,QAAL,CAAc,KAHvC,EAIE;AACA,aAAK,WAAL,GAAmB,KAAnB;AACA,aAAK,OAAL,CAAa,OAAb,GAAuB,KAAvB;AACF;AACD,KA9EM;AA+EH,IAAA,WA/EG,yBA+EW;AAChB,WAAK,KAAL,GAAa,IAAb;AACD;AAjFM;AA5BI,CAAf","sourcesContent":["<template>\n    <base-dialog\n      :show=\"!!error\"\n      title=\"An error occurred!\"\n      @close=\"handleError\"\n      >{{ error }}</base-dialog\n    >\n\n  <base-dialog :show=\"isLoading\" fixed title=\"Authenticating...\">\n    <base-spinner></base-spinner>\n  </base-dialog>\n\n  <base-card>\n    <form @submit.prevent=\"formSubmit\">\n      <div class=\"form-control\">\n        <label for=\"name\">Name</label>\n        <input\n          type=\"text\"\n          id=\"name\"\n          v-model.trim=\"name.value\"\n          @blur=\"nameValidation\"\n        />\n        <p v-if=\"!name.isValid\">Please enter your name</p>\n      </div>\n\n      <div class=\"form-control\">\n        <label for=\"surname\">Surname</label>\n        <input\n          type=\"text\"\n          id=\"surname\"\n          v-model.trim=\"surname.value\"\n          @blur=\"surnameValidation\"\n        />\n        <p v-if=\"!surname.isValid\">Please enter your surname</p>\n      </div>\n\n      <div class=\"form-control\">\n        <label for=\"email\">E-Mail</label>\n        <input\n          type=\"email\"\n          id=\"email\"\n          v-model.trim=\"email.value\"\n          @blur=\"emailValidation\"\n        />\n        <p v-if=\"!email.isValid\">Please enter a valid email</p>\n      </div>\n      <div class=\"form-control\">\n        <label for=\"password\">Password</label>\n        <input\n          type=\"password\"\n          id=\"password\"\n          v-model=\"password.value\"\n          @blur=\"passwordValidation\"\n        />\n        <p v-if=\"!password.isValid\">Please enter a valid password</p>\n      </div>\n      <div class=\"form-control\">\n        <label for=\"confirm\">Confirm Password</label>\n        <input\n          type=\"password\"\n          id=\"confirm\"\n          v-model=\"confirm.value\"\n          @blur=\"confirmValidation\"\n        />\n        <p v-if=\"!confirm.isValid\">The password doesn't match!</p>\n      </div>\n\n      <base-button>Register</base-button>\n    </form>\n  </base-card>\n</template>\n\n\n\n\n\n<script>\nexport default {\n  data() {\n    return {\n      name: {\n        value: \"\",\n        isValid: true,\n      },\n      surname: {\n        value: \"\",\n        isValid: true,\n      },\n      email: {\n        value: \"\",\n        isValid: true,\n      },\n      password: {\n        value: \"\",\n        isValid: true,\n      },\n      confirm: {\n        value: \"\",\n        isValid: true,\n      },\n      isFormValid: true,\n      isLoading: false,\n      error: null,\n    };\n  },\n  methods: {\n    async formSubmit() {\n      this.nameValidation();\n      this.surnameValidation();\n      this.emailValidation();\n      this.passwordValidation();\n      this.confirmValidation();\n      if (!this.isFormValid) {\n        return;\n      }\n      this.isLoading = true;\n      const actionPayload = {\n        name: this.name.value,\n        surname: this.surname.value,\n        email: this.email.value,\n        password: this.password.value,\n      };\n\n      // do the post\n      try {\n        await this.$store.dispatch(\"register\", actionPayload);\n        const redirecturl = \"/\" + (this.$route.query.redirect || \"public\");\n        this.$router.replace(redirecturl);\n      } catch (error) {\n        if (error)\n          this.error =\n            \"User with email: \" + actionPayload.email + \" already exists\" ||\n            \"Failed to register\";\n        else this.error = \"Failed to register\";\n\n      }\n      this.isLoading = false;\n    },\n\n    nameValidation() {\n      this.isFormValid = true;\n      this.name.isValid = true;\n      if (this.name.value === \"\") {\n        this.isFormValid = false;\n        this.name.isValid = false;\n      }\n    },\n    surnameValidation() {\n      this.isFormValid = true;\n      this.surname.isValid = true;\n      if (this.surname.value === \"\") {\n        this.isFormValid = false;\n        this.surname.isValid = false;\n      }\n    },\n\n    emailValidation() {\n      this.email.isValid = true;\n      this.isFormValid = true;\n      if (!this.email.value || !this.email.value.includes(\"@\")) {\n        this.email.isValid = false;\n        this.isFormValid = false;\n      }\n    },\n    passwordValidation() {\n      this.isFormValid = true;\n      this.password.isValid = true;\n      if (!this.password.value || this.password.value.length < 4) {\n        this.isFormValid = false;\n        this.password.isValid = false;\n      }\n    },\n    confirmValidation() {\n      this.isFormValid = true;\n      this.confirm.isValid = true;\n      if (\n        !this.confirm.value ||\n        this.confirm.value < 4 ||\n        this.confirm.value !== this.password.value\n      ) {\n        this.isFormValid = false;\n        this.confirm.isValid = false;\n      }\n    },\n        handleError() {\n      this.error = null;\n    },\n  },\n};\n</script>\n\n\n\n\n\n\n\n<style scoped>\nform {\n  margin: 1rem;\n  border: 1px solid #ccc;\n\n  padding: 1rem;\n}\n\n.form-control {\n  margin: 0.5rem 0;\n}\n\nlabel {\n  font-weight: bold;\n  margin-bottom: 0.5rem;\n  display: block;\n}\n\ninput,\ntextarea {\n  display: block;\n  width: 100%;\n  font: inherit;\n  border: 1px solid #ccc;\n  padding: 0.15rem;\n}\n\ninput:focus,\ntextarea:focus {\n  border-color: #3d008d;\n  background-color: #faf6ff;\n  outline: none;\n}\n</style>"],"sourceRoot":""}]}